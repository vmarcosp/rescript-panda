{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "persistent": true,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "^dev"
      ]
    }
  }
}
